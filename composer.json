{
  "name": "tinywan/casbin",
  "description": "Webman Casbin Plugin",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "casbin/casbin": "^3.20",
    "topthink/think-orm": "^2.0",
    "php-di/php-di": "^6.3",
    "workerman/redis": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tinywan\\Casbin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tinywan\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0|~9.0",
    "php-coveralls/php-coveralls": "^2.1",
    "workerman/webman": "^1.0"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer//"
    }
  }
}
